{
  "name": "codeinchq/lib-session",
  "version": "1.3.0",
  "description": "Simple session management library written in PHP 7",
  "homepage": "https://github.com/codeinchq/lib-session",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-server-handler": "^1.0",
    "hansott/psr7-cookies": "^1.0",
    "codeinchq/lib-psr15middlewares": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "guzzlehttp/psr7": "^1.4"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Session\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Session\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "suggest": {
    "codeinchq/lib-doctrinesessionhandler": "Provides a session handler to store the session data in a database using Doctrine"
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/codeinchq/lib-psr15middlewares" }
  ]
}